{
  "name": "toolsx",
  "version": "0.0.2",
  "description": "Tools X",
  "type": "module",
  "scripts": {
    "build": "unbuild",
    "play:nuxt": "pnpm run -r --filter=./playground/nuxt dev",
    "play:vite": "pnpm run -r --filter=./playground/vite dev",
    "lint": "oxlint --fix",
    "release": "pnpm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zxm965/tools.git"
  },
  "keywords": [
    "lib",
    "common"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./shared": {
      "import": "./dist/shared/index.mjs",
      "types": "./dist/shared/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.mjs",
  "author": "zxm965",
  "license": "MIT",
  "packageManager": "pnpm@10.18.2",
  "devDependencies": {
    "@types/node": "latest",
    "oxlint": "^1.22.0",
    "unbuild": "^3.6.1"
  }
}
