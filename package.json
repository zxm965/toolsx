{
  "name": "toolsx",
  "version": "0.0.1",
  "description": "Tools X",
  "type": "module",
  "scripts": {
    "build": "unbuild",
    "play:nuxt": "pnpm run -r --filter=./playground/nuxt dev",
    "play:vite": "pnpm run -r --filter=./playground/vite dev",
    "lint": "oxlint --fix",
    "release:shared": "pnpm run -r --filter=./packages/shared release",
    "release:utils": "pnpm run -r --filter=./packages/utils release",
    "release": "pnpm run release:shared && pnpm run release:utils"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zxm965/tools.git"
  },
  "keywords": [
    "lib",
    "common"
  ],
  "files": [
    "dist"
  ],
  "author": "zxm965",
  "license": "MIT",
  "packageManager": "pnpm@10.18.2",
  "devDependencies": {
    "@types/node": "latest",
    "oxlint": "^1.22.0",
    "unbuild": "^3.6.1"
  }
}
